<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whiteGreen"
    tools:context=".PlaceView">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="356dp"
        android:layout_height="298dp"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="127dp" />

    <TextView
        android:id="@+id/textViewPlaceName"
        android:layout_width="366dp"
        android:layout_height="75dp"
        android:fontFamily="sans-serif-medium"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="@color/darkGreen"
        android:textSize="40sp"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="28dp" />

    <Button
        android:id="@+id/zoomIN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="+"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="667dp" />

    <Button
        android:id="@+id/zoomOUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="-"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="249dp"
        tools:layout_editor_absoluteY="667dp" />

    <Button
        android:id="@+id/hybridBUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="HYBRID"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="575dp" />

    <Button
        android:id="@+id/normalBUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="NORMAL"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="598dp" />

    <Button
        android:id="@+id/satelliteBUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="SATELLITE"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="585dp" />

    <Button
        android:id="@+id/terrainBUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="TERRAIN"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="497dp" />

    <Button
        android:id="@+id/tusBUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="TU Sofia"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="490dp" />

    <Button
        android:id="@+id/BACK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#359c5e"
        android:padding="8dp"
        android:text="Back"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="490dp" />

</androidx.constraintlayout.widget.ConstraintLayout>